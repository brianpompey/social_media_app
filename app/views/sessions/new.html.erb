<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag session_path do |f| %>

    <div class="field">
    <%= label_tag :email, "Email:" %>
    <br>
    <%= text_field_tag :email %>
    </div>

    <div class="field">
    <%= label_tag :password, "Password:" %>
    <br>
    <%= password_field_tag :password %>
    </div>

    <div class="actions">
    <%= submit_tag "Log In" %>
    </div>

<% end %>